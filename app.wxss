/**app.wxss**/
@import "/wxParse/wxParse.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.kefu {
  position: fixed;
  bottom: 65%;
  margin-right: 30rpx;
  right: 0;
  margin-bottom: 20rpx;
  background: rgba(255, 255, 255, 0);
  border-radius:50%;
  width: 100rpx;
  height: 100rpx;
}

.gd {
  display:block;
  width:100% !important;
  height: 106.959px !important;
  overflow:hidden;
  pointer-events:auto;
  background-repeat:round;
}